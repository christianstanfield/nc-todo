<h2> <%= @user.id %> </h2>
<h2> <%= @user.email %> </h2>
<h2> <%= @user.api_token %> </h2>
<%= link_to 'Create a Todo', new_user_todo_path(current_user) %>

<h2> Your todos: </h2>
<% @user.todos.each do |todo| %>
<h3><input type="checkbox" name="some_name" value=""> <%= link_to 'Edit', edit_user_todo_path(current_user, todo) %> <%= todo.description %></h3>
<% end %>

